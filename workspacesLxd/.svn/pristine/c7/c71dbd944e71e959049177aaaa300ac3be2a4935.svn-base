    <sql id="selectTicketRecordVo">
        select id, pro_no, pro_name, contract_id, contract_no, contract_name,payinfoid ,payment_period, invoice_date, invoice_code, invoice_no, invoice_money, taxrate, invoice_nomoney, opposite_name, opposite_bank, opposite_bank_id, opposite_contacts, opposite_phone, opposite_address, invoice_voucher, unit_qualification, status, statusps, del_flag, remark, create_by, create_time, update_by, update_time from ct_bu_ticket_record
