            <if test="status != null "> and status = #{status}</if>
            <if test="statusps != null  and statusps != ''"> and statusps = #{statusps}</if>
        </where>
    </select>    
    <select id="selectTicketRecordById" parameterType="String" resultMap="TicketRecordResult">
