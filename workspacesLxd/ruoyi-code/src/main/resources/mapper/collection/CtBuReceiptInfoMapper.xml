    <select id="selectHxCtBuReceiptInfoList" parameterType="CtBuReceiptInfo" resultMap="CtBuReceiptInfoResult_">
        select
        i.dept_name,i.dept_id,
        i.id,
        i.approval_id,
        i.pro_no,
        i.pro_name,
        i.contract_id,
        i.contract_no,
        i.contract_name,
        i.invoice_id,
        i.invoice_code,
        i.invoice_no,
        i.collection_time,
        i.collection_amount,
        i.document_number,
        i.payment_method,
        i.voucher,
        i.req_id,
        ctBuInvoiceInfoVo.id ids , ctBuInvoiceInfoVo.contract_category contractCategorys, ctBuInvoiceInfoVo.req_id reqIds, ctBuInvoiceInfoVo.invoice_code invoiceCodes, ctBuInvoiceInfoVo.invoice_no invoiceNos, ctBuInvoiceInfoVo.invoice_date invoiceDates, ctBuInvoiceInfoVo.invoice_drawer invoiceDrawers, ctBuInvoiceInfoVo.invoice_reviewer invoicereviewers, ctBuInvoiceInfoVo.invoice_payee invoicePayees, ctBuInvoiceInfoVo.invoice_amount_tax invoiceAmountTaxs, ctBuInvoiceInfoVo.invoice_taxrate invoiceTaxrates, ctBuInvoiceInfoVo.invoice_amount_ntax invoiceAmountNtaxs,
        ctBuInvoiceInfoVo.status  statuss
        from ct_bu_receipt_info i  join ct_bu_invoice_info ctBuInvoiceInfoVo on ctBuInvoiceInfoVo.id = i.invoice_id

        <where>
            i.del_flag = 1  and  i.`status`=2
            <if test="createBy != null  and createBy != ''"> and i.create_by = #{createBy}</if>

            <if test="proName != null">and i.pro_name like concat('%', #{proName}, '%') </if>
            <if test="contractName != null">and i.contract_name  like concat('%', #{contractName}, '%')  </if>

            <if test="createBy != null  and createBy != ''"> and i.create_by = #{createBy}</if>

            <if test="invoiceCode != null  and invoiceCode != ''"> and ctBuInvoiceInfoVo.invoice_code = #{invoiceCode}</if>
            <if test="invoiceNo != null  and invoiceNo != ''"> and ctBuInvoiceInfoVo.invoice_no = #{invoiceNo}</if>
            <if test="createTime != null">and ctBuInvoiceInfoVo.create_time= #{createTime}</if>
            <if test="documentNumber != null  and documentNumber != ''"> and i.document_number = #{documentNumber}</if>
            <if test="paymentMethod != null  and paymentMethod != ''"> and i.payment_method = #{paymentMethod}</if>
            <if test="payType != null  and payType != ''"> and i.pay_type = #{payType}</if>

        </where>


    </select>

    <select id="selectNewCtBuReceiptInfoList" parameterType="CtBuReceiptInfo" resultMap="CtBuReceiptInfoResult">
        select i.*,
        f.id ids , f.contract_category contractCategorys, f.req_id reqIds, f.invoice_code invoiceCodes, f.invoice_no invoiceNos, f.invoice_date invoiceDates, f.invoice_drawer invoiceDrawers, f.invoice_reviewer invoicereviewers, f.invoice_payee invoicePayees, f.invoice_amount_tax invoiceAmountTaxs, f.invoice_taxrate invoiceTaxrates, f.invoice_amount_ntax invoiceAmountNtaxs,
        f.status  statuss,
        r.id id_, r.approval_type approvalType_, r.approval_id approvalId_, r.pro_no proNo_, r.pro_name proName_, r.contract_id contractId_, r.contract_no contractNo_, r.contract_name contractName_,
        r.invoice_type invoiceType_, r.req_deptid reqDeptid_, r.req_deptname reqDeptname_, r.req_userid reqUserid_, r.req_username reqUsername_, r.req_date reqDate_, r.payer_name payerName_,
        r.credit_code creditCode_, r.payer_address payerAddress_, r.payer_phone payerPhone_, r.payer_opening_bank payerOpeningBank_, r.payer_account payerAccount_, r.invoice_amount_tax invoiceAmountTax_,
        r.invoice_amount_ntax invoiceAmountNtax_, r.status status_,r.isOldCon
        from ct_bu_invoice_req r left join ct_bu_invoice_info f on r.id = f.req_id
        left join ct_bu_receipt_info i on f.id=i.invoice_id
        left join sys_dept d on r.req_deptid = d.dept_id
        <where>
            r.del_flag = 1  and  r.`status`=2  and f.del_flag=1
            and (i.pay_type=2 or i.pay_type is null)
            <if test="createBy != null  and createBy != ''"> and r.create_by = #{createBy}</if>
            <if test="status != null  and status != ''"> and f.status &gt;=  #{status}</if>
            <if test="id_ != null  and id_ != ''"> and r.id = #{id_}</if>

            <if test="proName != null">and r.pro_name like concat('%', #{proName}, '%') </if>
            <if test="contractName != null">and r.contract_name  like concat('%', #{contractName}, '%')  </if>

            <if test="createBy != null  and createBy != ''"> and i.create_by = #{createBy}</if>
            <if test="status != null  and status != ''"> and f.status &gt;= #{status}</if>

            <if test="invoiceCode != null  and invoiceCode != ''"> and f.invoice_code = #{invoiceCode}</if>
            <if test="invoiceNo != null  and invoiceNo != ''"> and f.invoice_no = #{invoiceNo}</if>
            <if test="createTime != null">and f.create_time= #{createTime}</if>
            <if test="documentNumber != null  and documentNumber != ''"> and i.document_number = #{documentNumber}</if>
            <if test="paymentMethod != null  and paymentMethod != ''"> and i.payment_method = #{paymentMethod}</if>
            <if test="payType != null  and payType != ''"> and i.pay_type = #{payType}</if>
            <if test="ids != null  and ids != ''"> and f.id = #{ids}</if>

            ${params.dataScope}
        </where>


    </select>
    <select id="selectCtBuReceiptInfoList" parameterType="CtBuReceiptInfo" resultMap="CtBuReceiptInfoResult">
<!--        <include refid="selectCtBuReceiptInfoVo"/>-->
        SELECT
        pay_Type_Name,
        pay_Type,
        i.id,
        i.approval_id,
        r.pro_no,
        if(r.pro_name is null,i.pro_name,r.pro_name) proName_,
        r.contract_id,
        r.contract_no,
        if(r.contract_name is null,i.contract_name,r.contract_name) contractName_,
        r.invoice_type invoiceType_,
        r.req_deptname reqDeptname_,
        r.req_username reqUsername_,
        r.invoice_amount_tax invoiceAmountTax_,
        r.req_date reqDate_,
        r.payer_name payerName_,
        invoice_id,
        invoice_code,
        invoice_no,
        collection_time,
        collection_amount,
        document_number,
        payment_method,
        voucher,
        i.STATUS,
        i.statusPS,
        i.del_flag,
        i.remark,
        i.create_by,
        i.create_time,
        i.update_by,
        i.update_time,
        i.instance_id,
        i.dept_name,i.dept_id
        FROM
        ct_bu_receipt_info i left join ct_bu_invoice_req r on i.id=r.yskid
        <where>
            i.del_flag = 1
            <if test="approvalId != null  and approvalId != ''"> and i.approval_id = #{approvalId}</if>
            <if test="proNo != null  and proNo != ''"> and r.pro_no = #{proNo}</if>
            <if test="proName != null  and proName != ''"> and (r.pro_name like concat('%', #{proName}, '%') or i.pro_name like concat('%', #{proName}, '%'))</if>
            <if test="contractId != null  and contractId != ''"> and r.contract_id = #{contractId}</if>
            <if test="contractNo != null  and contractNo != ''"> and r.contract_no = #{contractNo}</if>
            <if test="contractName != null  and contractName != ''"> and (r.contract_name like concat('%', #{contractName}, '%') or (i.contract_name like concat('%', #{contractName}, '%')))</if>
            <if test="invoiceId != null  and invoiceId != ''"> and i.invoice_id = #{invoiceId}</if>
            <if test="invoiceCode != null  and invoiceCode != ''"> and (i.invoice_code = #{invoiceCode})</if>
            <if test="invoiceNo != null  and invoiceNo != ''"> and i.invoice_no = #{invoiceNo}</if>
            <if test="collectionTime != null "> and i.collection_time = #{collectionTime}</if>
            <if test="collectionAmount != null "> and i.collection_amount = #{collectionAmount}</if>
            <if test="documentNumber != null  and documentNumber != ''"> and document_number = #{documentNumber}</if>
            <if test="paymentMethod != null  and paymentMethod != ''"> and payment_method = #{paymentMethod}</if>
            <if test="voucher != null  and voucher != ''"> and voucher = #{voucher}</if>
            <if test="status != null "> and i.status = #{status}</if>
            <if test="statusps != null  and statusps != ''"> and i.statusPS = #{statusps}</if>
            <if test="instanceId != null  and instanceId != ''"> and i.instance_id = #{instanceId}</if>
            <if test="payType != null  and payType != ''"> and i.pay_type = #{payType}</if>

        </where>
    </select>



