<script>
import { listContract, getContract, delContract, addContract, updateContract, exportContract } from "@/api/contract/contractInfo";
import selectObjectDialog from './selectObjectDialog'
export default {
  name: "Contract",
  components:{
    selectObjectDialog
  },
  data() {
    return {
      // 遮罩层
      loading: true,
      // 选中数组
      ids: [],
      // 非单个禁用
      single: true,
      // 非多个禁用
      multiple: true,
      // 显示搜索条件
      showSearch: true,
      // 总条数
      total: 0,
      // 合同信息录入主表格数据
      contractList: [],
      projectCategoryTypeList: [],
      contentTypeList:[],
      contractCategoryTypeList: [],
      incomeTypeList: [],
      payMethodList: [],
      contractStatusList: [],
      // 日期范围
      starttime: [],
      endtime: [],
      contractServeStart: [],
      contractServeEnd: [],
      // 弹出层标题
