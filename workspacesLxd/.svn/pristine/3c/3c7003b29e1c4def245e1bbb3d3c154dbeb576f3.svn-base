      open: false,
      // 查询参数
      queryParams: {
        pageNum: 1,
        pageSize: 10,
        projectCategory: null,
        contentType: null,
        projectId: null,
        projectNo: null,
        projectName: null,
        contractNo: null,
        contractName: null,
        month: null,
        contractCategory: null,
        incomeType: null,
        oppositeName: null,
        oppositeBank: null,
        oppositeBankId: null,
