    <sql id="selectCtBuReceiptInfoVo">
        select id, approval_id, pro_no, pro_name, contract_id, contract_no, contract_name, invoice_id, invoice_code, invoice_no, collection_time, collection_amount, document_number, payment_method, voucher, status, statusPS, del_flag, remark, create_by, create_time, update_by, update_time from ct_bu_receipt_info
    </sql>

    <select id="selectCtBuReceiptInfoList" parameterType="CtBuReceiptInfo" resultMap="CtBuReceiptInfoResult">
        <include refid="selectCtBuReceiptInfoVo"/>
        <where>
            del_flag = 1
            <if test="approvalId != null  and approvalId != ''"> and approval_id = #{approvalId}</if>
            <if test="proNo != null  and proNo != ''"> and pro_no = #{proNo}</if>
            <if test="proName != null  and proName != ''"> and pro_name like concat('%', #{proName}, '%')</if>
            <if test="contractId != null  and contractId != ''"> and contract_id = #{contractId}</if>
            <if test="contractNo != null  and contractNo != ''"> and contract_no = #{contractNo}</if>
            <if test="contractName != null  and contractName != ''"> and contract_name like concat('%', #{contractName}, '%')</if>
            <if test="invoiceId != null  and invoiceId != ''"> and invoice_id = #{invoiceId}</if>
            <if test="invoiceCode != null  and invoiceCode != ''"> and invoice_code = #{invoiceCode}</if>
            <if test="invoiceNo != null  and invoiceNo != ''"> and invoice_no = #{invoiceNo}</if>
            <if test="collectionTime != null "> and collection_time = #{collectionTime}</if>
            <if test="collectionAmount != null "> and collection_amount = #{collectionAmount}</if>
            <if test="documentNumber != null  and documentNumber != ''"> and document_number = #{documentNumber}</if>
            <if test="paymentMethod != null  and paymentMethod != ''"> and payment_method = #{paymentMethod}</if>
            <if test="voucher != null  and voucher != ''"> and voucher = #{voucher}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="statusps != null  and statusps != ''"> and statusPS = #{statusps}</if>
        </where>
    </select>
    
    <select id="selectCtBuReceiptInfoById" parameterType="String" resultMap="CtBuReceiptInfoResult">
        <include refid="selectCtBuReceiptInfoVo"/>
        where id = #{id} and del_flag = 1
    </select>
        
